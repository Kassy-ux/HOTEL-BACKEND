#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kassy/Desktop/teach2give/HOTEL-BOOKING-BACKEND/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.7_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_6c4f1ba97b995cdccf44133185c61765/node_modules/ts-jest/node_modules:/home/kassy/Desktop/teach2give/HOTEL-BOOKING-BACKEND/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.7_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_6c4f1ba97b995cdccf44133185c61765/node_modules:/home/kassy/Desktop/teach2give/HOTEL-BOOKING-BACKEND/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kassy/Desktop/teach2give/HOTEL-BOOKING-BACKEND/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.7_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_6c4f1ba97b995cdccf44133185c61765/node_modules/ts-jest/node_modules:/home/kassy/Desktop/teach2give/HOTEL-BOOKING-BACKEND/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.7_@jest+transform@29.7.0_@jest+types@30.0.0-beta.3_babe_6c4f1ba97b995cdccf44133185c61765/node_modules:/home/kassy/Desktop/teach2give/HOTEL-BOOKING-BACKEND/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
